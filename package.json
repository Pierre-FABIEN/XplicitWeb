{
	"name": "sveltekit-boilerplate",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "concurrently \"vite dev --port 1000\" \"npm run prettier:watch\" ",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"prettier:watch": "chokidar \"src/**/*.{js,ts,svelte,scss}\" -c \"npm run prettier:single -- {path}\"",
		"prettier:single": "prettier --write",
		"prettier": "prettier --write \"src/**/*.{js,ts,svelte,scss}\"",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"model-pipeline:run": "node scripts/model-pipeline.js",
		"seed": "node src/lib/server/seed.js",
		"reset": "node src/lib/server/resetDb.js"
	},
